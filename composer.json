    "amphp/dns": "dev-master as 0.9.9",
    "amphp/file": "dev-master as 0.3.3",
    "amphp/uri": "dev-master as 0.1.3",
    "vlucas/phpdotenv": "^2.4",
    "ext-json": "*",
    "ext-swoole": ">=4.1"
  },
  "require-dev": {
    "swoole/ide-helper": "dev-master"
  },
  "repositories": [
